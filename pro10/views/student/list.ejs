
<%- include('../header') %>
	
		
<!-- <a href="/student/demo/indore" class="btn btn-danger">Hello</a> -->
        
<div class="container" style="min-height:700px">
    <div class="row">
        <div class="col-md-12">
            <h3>List of All Student</h3>
            <table class="table table-dark table-hover table-striped">
                <tr>
                    <th>S.No.</th>
                    <th>Full Name</th>
                    
                    <th>Contact</th>
                    <th>Detail</th>
                </tr>

                <% stu.forEach((x, n)=>{ %>
                    <tr>
                        <td><%= n+1 %></td>
                        <td><%= x.full_name %></td>
                    
                        <td><%= x.contact %></td>
                        <td><a href="/student/detail/<%= x._id %>" class="btn btn-info btn-sm">Detail</a></td>
                    </tr>
                <% }) %>
            </table>
        </div>
    </div>

</div>



<%- include('../footer') %>